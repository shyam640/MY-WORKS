{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "C:\\MinGW\\**",
                "C:/MinGW/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "C:\\MinGW\\bin\\gcc.exe",
            "cStandard": "gnu17",
            "cppStandard": "gnu++20",
            "intelliSenseMode": "gcc-x86",
            "configurationProvider": "ms-vscode.cmake-tools",
            "browse": {
                "path": [
                    "C:\\MinGW",
                    "C:/MinGW",
                    "C:\\MinGW\\**"
                ]
            },
            "compilerArgs": [
                "-lbgi",
                "-lgdi32",
                "-Icomdlg32",
                "-luuid",
                "-loleaut32",
                "-lole32"
            ]
        }
    ],
    "version": 4
}